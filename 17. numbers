def generate_sequence(M, N, K):
    sequence = []
    current = M
    while current <= N:
        sequence.append(current)
        current += K
    return sequence

# Example usage
M = 50
N = 100
K = 7

sequence = generate_sequence(M, N, K)
print(", ".join(map(str, sequence)) + ",")

                                                                        
